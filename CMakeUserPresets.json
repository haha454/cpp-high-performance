{
  "version": 4,
  "include": [
    "build/Debug/generators/CMakePresets.json"
  ],
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 23,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "dev-common",
      "binaryDir": "${sourceDir}/build/Debug",
      "hidden": true,
      "inherits": [
        "dev-mode",
        "conan-debug"
      ],
      "cacheVariables": {
      }
    },
    {
      "name": "dev-release-common",
      "binaryDir": "${sourceDir}/build/Release",
      "hidden": true,
      "inherits": [
        "dev-mode",
        "conan-release"
      ],
      "cacheVariables": {
      }
    },
    {
      "name": "dev-linux",
      "inherits": [
        "dev-common",
        "ci-linux"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
      }
    },
    {
      "name": "dev-release-linux",
      "inherits": [
        "dev-release-common",
        "ci-linux"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
      }
    },
    {
      "name": "dev-darwin",
      "inherits": [
        "dev-common",
        "ci-darwin"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
      }
    },
    {
      "name": "dev-release-darwin",
      "inherits": [
        "dev-release-common",
        "ci-darwin"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
      }
    },
    {
      "name": "dev-win64",
      "binaryDir": "${sourceDir}/build/Debug",
      "inherits": [
        "dev-common",
        "ci-win64"
      ],
      "environment": {
        "UseMultiToolTask": "true",
        "EnforceProcessCountAcrossBuilds": "true"
      }
    },
    {
      "name": "dev-release-win64",
      "inherits": [
        "dev-release-common",
        "ci-win64"
      ],
      "environment": {
        "UseMultiToolTask": "true",
        "EnforceProcessCountAcrossBuilds": "true"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "dev-common",
      "hidden": true,
      "configuration": "Debug",
      "jobs": 10
    },
    {
      "name": "dev-release-common",
      "hidden": true,
      "configuration": "Release",
      "jobs": 10
    },
    {
      "name": "dev-linux",
      "inherits": "dev-common",
      "configurePreset": "dev-linux"
    },
    {
      "name": "dev-release-linux",
      "inherits": "dev-release-common",
      "configurePreset": "dev-release-linux"
    },
    {
      "name": "dev-darwin",
      "inherits": "dev-common",
      "configurePreset": "dev-darwin"
    },
    {
      "name": "dev-release-darwin",
      "inherits": "dev-release-common",
      "configurePreset": "dev-release-darwin"
    },
    {
      "name": "dev-win64",
      "inherits": "dev-common",
      "configurePreset": "dev-win64"
    },
    {
      "name": "dev-release-win64",
      "inherits": "dev-release-common",
      "configurePreset": "dev-release-win64"
    }
  ],
  "testPresets": [
    {
      "name": "dev-common",
      "hidden": true,
      "configuration": "Debug",
      "output": {
        "outputOnFailure": true
      },
      "execution": {
        "jobs": 10,
        "noTestsAction": "error"
      }
    },
    {
      "name": "dev-release-common",
      "hidden": true,
      "configuration": "Release",
      "output": {
        "outputOnFailure": true
      },
      "execution": {
        "jobs": 10,
        "noTestsAction": "error"
      }
    },
    {
      "name": "dev-linux",
      "inherits": "dev-common",
      "configurePreset": "dev-linux"
    },
    {
      "name": "dev-release-linux",
      "inherits": "dev-release-common",
      "configurePreset": "dev-release-linux"
    },
    {
      "name": "dev-darwin",
      "inherits": "dev-common",
      "configurePreset": "dev-darwin"
    },
    {
      "name": "dev-release-darwin",
      "inherits": "dev-release-common",
      "configurePreset": "dev-release-darwin"
    },
    {
      "name": "dev-win64",
      "inherits": "dev-common",
      "configurePreset": "dev-win64"
    },
    {
      "name": "dev-release-win64",
      "inherits": "dev-release-common",
      "configurePreset": "dev-release-win64"
    }
  ]
}
